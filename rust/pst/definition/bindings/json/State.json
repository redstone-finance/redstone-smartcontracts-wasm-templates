{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "State",
  "type": "object",
  "required": [
    "balances",
    "owner",
    "ticker"
  ],
  "properties": {
    "balances": {
      "type": "object",
      "additionalProperties": {
        "type": "integer",
        "format": "uint64",
        "minimum": 0.0
      }
    },
    "canEvolve": {
      "type": [
        "boolean",
        "null"
      ]
    },
    "evolve": {
      "type": [
        "string",
        "null"
      ]
    },
    "name": {
      "type": [
        "string",
        "null"
      ]
    },
    "owner": {
      "type": "string"
    },
    "ticker": {
      "type": "string"
    }
  }
}