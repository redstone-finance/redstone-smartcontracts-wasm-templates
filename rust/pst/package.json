{
  "name": "warp-erc1155",
  "version": "0.0.1",
  "description": "Rust implementation of the ERC1155 contract standard",
  "main": "index.js",
  "author": "Redstone Team <dev@redstone.finance>",
  "license": "MIT",
  "scripts": {
    "build": "wasm-pack build --target nodejs --release --out-name rust-contract implementation",
    "gen-bindings": "yarn gen-json && yarn gen-ts",
    "gen-json": "GENERATE_JSON=1 cargo test generate_json -- --nocapture",
    "gen-ts": "ts-node --transpileOnly ./scripts/generate-ts.ts && yarn lint:fix",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint 'definition/bindings/ts/*.ts' --fix",
    "prettier:format": "prettier --config .prettierrc '**/**/*.ts' --write"
  },
  "engines": {
    "node": ">=16.5"
  },
  "dependencies": {
    "arlocal": "^1.1.58",
    "json-schema-to-typescript": "^11.0.1",
    "typescript": "^4.6.2",
    "warp-contracts": "^1.2.13"
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "prettier": "^2.3.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1"
  }
}
